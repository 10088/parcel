{
  "name": "@parcel/repl",
  "private": true,
  "version": "1.12.0",
  "description": "Blazing fast, zero configuration web application bundler",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/parcel-bundler/parcel.git"
  },
  "scripts": {
    "start": "parcel src/index.html --no-source-maps",
    "dev": "NODE_ENV=development parcel build src/index.html --no-source-maps --no-content-hash --no-minify",
    "prod": "rm -rf dist/*; parcel build src/index.html --no-source-maps --no-content-hash",
    "prod:shake": "rm -rf dist/*; NODE_OPTIONS='--max-old-space-size=4096' parcel build src/index.html --experimental-scope-hoisting",
    "deploy": "yarn prod && now && now alias"
  },
  "dependencies": {
    "@parcel/fs": "^1.11.0",
    "@parcel/logger": "^1.11.0",
    "codemirror": "^5.46.0",
    "comlink": "^3.2.0",
    "jszip": "^3.2.1",
    "memory-fs": "^0.4.1",
    "parcel-bundler": "^1.12.1",
    "preact": "^10.0.0-beta.1",
    "react-codemirror2": "^6.0.0",
    "toml": "^3.0.0"
  },
  "devDependencies": {
    "@babel/core": "^7.4.4",
    "@babel/plugin-proposal-object-rest-spread": "^7.4.4",
    "@babel/preset-env": "^7.4.4"
  },
  "alias": {
    "react": "preact",
    "@iarna/toml": "toml",
    "deasync": false,
    "node-sass": "sass",
    "fs": "./memory-fs.js",
    "v8-compile-cache": false
  },
  "browserslist": [
    "Chrome 52",
    "Firefox 53",
    "Safari 10"
  ]
}
